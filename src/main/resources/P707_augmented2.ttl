##
## PersonName subclasses from ontology/core
##
bdo:PersonPrimaryName
  a owl:Class ;
  adm:technicalComment "what is the difference with :PersonCommonName"@en ;
  adm:userTooltip "The primary name by which a Person is known"@en ;
  rdfs:label "Primary Name"@en ;
  rdfs:subClassOf bdo:PersonName ;
.
bdo:PersonPrimaryTitle
  a owl:Class ;
  adm:userTooltip "The primary or common title by which a Person is known."@en ;
  rdfs:label "Primary Title"@en ;
  rdfs:subClassOf bdo:PersonName ;
.
bdo:PersonGterStonTitle
  a owl:Class ;
  adm:userTooltip "The title bestowed upon a Person who has discovered a terma"@en ;
  rdfs:label "Terton Title"@en ;
  rdfs:subClassOf bdo:PersonName ;
.

##
## Gender defns from ontology/core
##
bdo:Gender
  a owl:Class ;
  adm:userTooltip "The Gender Type records the gender of a :Person if known. If the gender is not known, then bdr:GenderNotSpecified is used."@en ;
  rdfs:label "Gender"@en ;
.
bdr:GenderFemale
  a bdo:Gender ;
  skos:prefLabel "female"@en ;
.
bdr:GenderMale
  a bdo:Gender ;
  skos:prefLabel "male"@en ;
.
bdr:GenderMixed
  a bdo:Gender ;
  skos:prefLabel "mixed gender"@en ;
  adm:translationPriority 2 ;
.
bdr:GenderNotSpecified
  a bdo:Gender ;
  skos:prefLabel "gender not specified"@en ;
.

